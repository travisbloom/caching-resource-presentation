{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "indent": [1, 4],
        "react/jsx-indent": [1, 4],
        "react/jsx-indent-props": [1, 4],
        "react/prop-types": 0,
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "react/no-unused-prop-types": 0,
        "no-confusing-arrow": 0,
        "global-require": 0,
        "class-methods-use-this": 0,
        "semi": [1, "never"]
    }
}
